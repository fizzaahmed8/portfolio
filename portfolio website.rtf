/* Base reset */\par
* \{\par
  margin: 0;\par
  padding: 0;\par
\}\par
\par
/* box-sizing and font sizing */\par
*,\par
*::before,\par
*::after \{\par
  box-sizing: inherit;\par
\}\par
\par
html \{\par
  box-sizing: border-box;\par
  font-size: 62.5%;\par
  scroll-behavior: smooth;\par
\}\par
\par
@media (max-width: 75em) \{\par
  html \{\par
    font-size: 60%;\par
  \}\par
\}\par
\par
@media (max-width: 61.25em) \{\par
  html \{\par
    font-size: 58%;\par
  \}\par
\}\par
\par
@media (max-width: 28.75em) \{\par
  html \{\par
    font-size: 55%;\par
  \}\par
\}\par
\par
/* Base styling */\par
body \{\par
  font-family: "Poppins", sans-serif;\par
  font-size: 1.8rem;\par
  font-weight: 400;\par
  line-height: 1.4;\par
  color: #fff;\par
\}\par
\par
img \{\par
  display: block;\par
  width: 100%;\par
\}\par
\par
h1,\par
h2 \{\par
  font-family: "Raleway", sans-serif;\par
  font-weight: 700;\par
  text-align: center;\par
\}\par
\par
h1 \{\par
  font-size: 6rem;\par
  font-size: 72px;\par
  background: -webkit-radial-gradient(#e8f5e9, #b2dfdb);\par
  -webkit-background-clip: text;\par
  -webkit-text-fill-color: transparent;\par
\}\par
\par
h2 \{\par
  font-size: 4.2rem;\par
  color: #2196f3;\par
  border: 0.5px solid #ddd;\par
\}\par
p \{\par
  font-size: 2rem;\par
  color: #e1bee7;\par
\}\par
.nav \{\par
  display: flex;\par
  justify-content: flex-end;\par
  position: fixed;\par
  top: 0;\par
  left: 0;\par
  width: 100%;\par
  height: auto;\par
  background: #3e4e88;\par
  box-shadow: 0 2px 0 rgba(0, 0, 0, 0.4);\par
  z-index: 10;\par
\}\par
\par
li \{\par
  display: inline;\par
\}\par
\par
ul \{\par
  list-style: none;\par
  padding: 10px;\par
\}\par
\par
a \{\par
  text-decoration: none;\par
  margin-left: 1.5em;\par
  margin-right: 1.5em;\par
  font-size: 1em;\par
  color: #fff;\par
\}\par
a:hover \{\par
  color: #fbc02d;\par
\}\par
#welcome-section \{\par
  display: flex;\par
  flex-direction: column;\par
  justify-content: center;\par
  align-items: center;\par
  width: 100%;\par
  height: 100vh;\par
  background: linear-gradient(90deg, #fc466b 0%, #3f5efb 100%);\par
\}\par
\par
/* Section 2*/\par
\par
.projects-section \{\par
  text-align: center;\par
  padding: 10rem 2rem;\par
  background: #eceff1;\par
\}\par
.projects-section-header \{\par
  max-width: 640px;\par
  margin: 3rem auto 6rem auto;\par
  border-bottom: 0 solid white;\par
  padding-top: 1rem;\par
  padding-bottom: 1rem;\par
  border-radius: 38px;\par
  word-spacing: 4px;\par
  letter-spacing: 3px;\par
  box-shadow: 0 5px 20px #ddd;\par
\}\par
@media (max-width: 28.75em) \{\par
  .projects-section-header \{\par
    font-size: 4rem;\par
  \}\par
\}\par
\par
.projects-grid \{\par
  display: grid;\par
  grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));\par
  grid-gap: 4rem;\par
  width: 100%;\par
  max-width: 1280px;\par
  margin: 0 auto;\par
  margin-bottom: 6rem;\par
\}\par
@media (max-width: 30.625em) \{\par
  .projects-section \{\par
    padding: 6rem 1rem;\par
  \}\par
\par
  .projects-grid \{\par
    grid-template-columns: 1fr;\par
  \}\par
\}\par
.project \{\par
  background: #ad1457;\par
  box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.5);\par
  border-radius: 28px;\par
  box-shadow: 0 2.8px 2.2px rgba(0, 0, 0, 0.034),\par
    0 6.7px 5.3px rgba(0, 0, 0, 0.048), 0 12.5px 10px rgba(0, 0, 0, 0.06),\par
    0 22.3px 17.9px rgba(0, 0, 0, 0.072), 0 41.8px 33.4px rgba(0, 0, 0, 0.086),\par
    0 100px 80px rgba(0, 0, 0, 0.12);\par
  transition: all 2s, transform 0.5s, box_shadow 0.5s;\par
\}\par
.project:hover \{\par
  box-shadow: 6px 6px 20px rgba(0, 0, 0, 0.6);\par
  transform: scale(1.08);\par
\}\par
.contact-section \{\par
  display: flex;\par
  flex-direction: column;\par
  justify-content: center;\par
  align-items: center;\par
  text-align: center;\par
  width: 100%;\par
  height: 80vh;\par
  padding: 0 2rem;\par
  background: #363748;\par
\}\par
.contact-section-header > h2 \{\par
  font-size: 6rem;\par
\}\par
#con \{\par
  color: #fff;\par
  border: none;\par
\}\par
@media (max-width: 28.75em) \{\par
  .contact-section-header > h2 \{\par
    font-size: 4rem;\par
  \}\par
\}\par
.contact-links \{\par
  display: flex;\par
  justify-content: center;\par
  width: 100%;\par
  max-width: 980px;\par
  margin-top: 4rem;\par
  flex-wrap: wrap;\par
\}\par
.contact-details \{\par
  font-size: 2.4rem;\par
  text-shadow: 2px 2px 1px #1f1f1f;\par
  transition: transform 0.3s ease-out;\par
\}\par
.contact-details:hover \{\par
  transform: translateY(8px);\par
  color: red;\par
\}\par
.form \{\par
  max-width: 640px;\par
  margin-top: 2rem;\par
  display: block;\par
  margin-left: auto;\par
  margin-right: auto;\par
\}\par
#email \{\par
  max-width: 300px;\par
  width: 300px;\par
  padding-top: 12px;\par
  padding-bottom: 12px;\par
  padding-right: 1rem;\par
  padding-left: 1rem;\par
  margin-top: 2rem;\par
  border-radius: 28px;\par
  text-align: center;\par
  outline: none;\par
\}\par
#submit \{\par
  margin-top: 1rem;\par
  background-color: red;\par
  border-radius: 28px;\par
  padding-top: 0.7rem;\par
  padding-right: 2.1rem;\par
  padding-bottom: 1.1rem;\par
  transition: all 2s, transform 0.5s, box_shadow 0.5s;\par
\}\par
#submit:hover \{\par
  box-shadow: 6px 6px 20px rgba(0, 0, 0, 0.6);\par
  transform: scale(1.04);\par
\}\par
\par
/* Footer Section */\par
footer \{\par
  font-weight: 300;\par
  display: flex;\par
  justify-content: space-evenly;\par
  padding: 2rem;\par
  background: #0c344e;\par
  border-top: 4px solid #bfc8ea;\par
\}\par
#foot \{\par
  color: #fff;\par
  font-family: "Poppins", sans-serif;\par
\}\par
#fot \{\par
  margin-left: -0.1rem;\par
  color: #ff9f5c;\par
  font-family: "Poppins", sans-serif;\par
\}\par
#fot:hover \{\par
  color: #f31702;\par
\}\par
}
